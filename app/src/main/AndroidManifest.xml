        </activity>
        <activity android:name=".DecoMainActivity">

        </activity>
        <activity android:name=".ProductActivity">

        </activity>
        <activity android:name=".ShopActivity"/>
        <activity android:name=".DataContactActivity"/>
        <activity android:name=".FinishComandActivity"/>
        <activity android:name=".ThankYouActivity"/>
